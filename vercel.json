{
  "version": 2,
  "builds": [
    { "src": "frontend/package.json", "use": "@vercel/astro" },
    { "src": "api/index.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/index.js" },
    { "src": "/proxy/(.*)", "dest": "/api/index.js" },
    { "src": "/(.*)", "dest": "frontend/dist/$1" }
  ]
}
